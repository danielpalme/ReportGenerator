{
    "manifestVersion": 1,
    "id": "reportgenerator",
    "name": "ReportGenerator",
    "version": "4.4.0",
    "publisher": "Palmmedia",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "ReportGenerator converts coverage reports generated by OpenCover, dotCover, Visual Studio, NCover, Cobertura, JaCoCo, Clover, gcov or lcov into human readable reports in various formats.",
    "categories": [
        "Build and release"
    ],
    "tags": ["Code Coverage", "Reporting", "Testing", "TDD"],
    "screenshots": [
        {
            "path": "img/screenshot1.png"
        },
        {
            "path": "img/screenshot2.png"
        }
    ],
    "icons": {
        "default": "icon.png"
    },
    "files": [
        {
            "path": "ReportGenerator"
        },
        {
          "path": "img",
          "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://danielpalme.github.io/ReportGenerator/usage.html"
        },
        "source": {
            "uri": "https://github.com/danielpalme/ReportGenerator"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/danielpalme/ReportGenerator"
    },
    "contributions": [
        {
            "id": "reportgenerator-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "reportgenerator"
            }
        }
    ]
}